{
    "version": 2,
    "builds": [
      {
        "src": "*.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/"
      }
    ],
    
        "functions": {
          "api/**/*.js": {
            "timeout": 1000
          }
        }
      
      
  }